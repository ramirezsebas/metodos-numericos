   "source": [
    "f = {2010:20500, 2012:22600, 2016:26400}\n",
    "\n",
    "def dif_div(f, i, k=None):\n",
    "    x = [*f.keys()]\n",
    "    if k is None or i == k:\n",
    "        return f[x[i]]\n",
    "    else:\n",
    "        return (dif_div(f, i+1, k) - dif_div(f, i, k-1)) / (x[k] - x[i])\n",
    "\n",
    "dif_div(f, 0, 2)"
   ]
